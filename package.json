{
  "name": "@tripsit/http-api",
  "private": true,
  "version": "0.1.0",
  "main": "./index.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node --transpile-only .",
    "dev": "NODE_ENV=development nodemon --exec \"node --require ts-node/register --inspect\" ./index.ts",
    "setup": "npm run generate-app-tokens",
    "test": "npm run jest:unit && npm run jest:integration",
    "lint": "eslint --fix --ext .ts,.js .",
    "jest:unit": "NODE_ENV=test jest -c jest.unit.config.ts",
    "jest:integration": "NODE_ENV=test jest -ic jest.integration.config.ts",
    "migrate": "knex migrate:latest --update-env",
    "migrate:make": "knex migrate:make bridges -x ts --",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make -x ts --",
    "update-tb-db": "ts-node --transpile-only ./scripts/updateTBDBTypes.ts",
    "generate-app-tokens": "ts-node --transpile-only ./scripts/generate-app-tokens.ts",
    "reset-db": "knex migrate:rollback --all && knex migrate:latest && npm run db-types && npm run lint && npm run update-tb-db && npm run seed ",
    "db-types": "ts-node --transpile-only ./type-output/types.ts",
    "sonar-scanner": "sonar-scanner"
  },
  "authors": [
    "SevenCats <sevencats@tripsit.me>",
    "Moonbear <moonbear@tripsit.me>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@apollo/server": "^4.0.2",
    "@discordjs/rest": "^1.3.0",
    "@graphql-tools/schema": "^9.0.4",
    "@graphql-tools/utils": "^8.12.0",
    "apollo-datasource": "^3.3.2",
    "argon2": "^0.30.1",
    "datasource-sql": "^2.0.1",
    "discord-api-types": "^0.37.17",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.6",
    "handlebars": "^4.7.7",
    "knex": "^2.4.2",
    "luxon": "^3.0.4",
    "minimatch": "^3.0.4",
    "mjml": "^4.13.0",
    "nodemailer": "^6.8.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/luxon": "^3.0.2",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.11.5",
    "@types/nodemailer": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "jest": "^29.3.1",
    "knex-types": "^0.4.0",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
